{
  "name": "mern-app",
  "version": "1.0.0",
  "description": "Mern stack",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ScobarDen/mern-app.git"
  },
  "keywords": [
    "mern",
    "react"
  ],
  "author": "ScobarDen <gdv-ps@mail.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ScobarDen/mern-app/issues"
  },
  "homepage": "https://github.com/ScobarDen/mern-app#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.5",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
